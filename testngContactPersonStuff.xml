<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestNgContactPersonStuff">
    <listeners>
        <listener class-name="resources.ExtentReporterNG"/>
        <listener class-name="resources.Listeners"/>
    </listeners>

    <test name="Registration Test">
        <classes>
            <class name="MulRegister"/>
        </classes>
    </test>

    <test name="Verify Test">
        <classes>
            <class name="Verify"/>
        </classes>
    </test>

    <test name="Creating an ambulant Organisation">
        <classes>
            <class name="departments.CreateAmbulantOrganisation"/>
        </classes>
    </test>

    <test name="Creating a Contact Person">
        <classes>
            <class name="contactPerson.CreateContactPerson"/>
        </classes>
    </test>

    <test name="Edit a Contact Person">
        <classes>
            <class name="contactPerson.EditContactPerson"/>
        </classes>
    </test>

    <test name="Invite a Contact Person">
        <classes>
            <class name="contactPerson.InviteContactPerson"/>
        </classes>
    </test>

    <test name="Contact Person gets Department">
        <classes>
            <class name="contactPerson.ContactPersonGetDepartment"/>
        </classes>
    </test>

    <test name="Delete a Contact Person">
        <classes>
            <class name="contactPerson.DeleteContactPerson"/>
        </classes>
    </test>

    <test name="Delete Placement">
        <classes>
            <class name="placements.DeletePlacement"/>
        </classes>
    </test>

    <test name="Delete User">
        <classes>
            <class name="DeleteUser"/>
        </classes>
    </test>
</suite>