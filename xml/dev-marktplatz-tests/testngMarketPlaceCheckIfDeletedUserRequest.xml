<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Check if deleted user request is available">
    <listeners>
        <listener class-name="resources.ExtentReporterNG"/>
        <listener class-name="resources.Listeners"/>
    </listeners>

    <test name="Marketplace Registration">
        <classes>
            <class name="marketplaceTests.MarketplaceRegister"/>
        </classes>
    </test>

    <test name="Verify Marketplace User">
        <classes>
            <class name="marketplaceTests.VerifyMarketplaceUser"/>
        </classes>
    </test>

    <test name="Ask for Meeting as User">
        <classes>
            <class name="marketplaceTests.AskForMeeting"/>
        </classes>
    </test>

    <test name="Delete Marketplace User">
        <classes>
            <class name="marketplaceTests.DeleteMarketplaceUser"/>
        </classes>
    </test>

    <test name="Check if deleted Meeting Request are available">
        <classes>
            <class name="departments.CheckIfRequestAreDeleted"/>
        </classes>
    </test>
</suite>